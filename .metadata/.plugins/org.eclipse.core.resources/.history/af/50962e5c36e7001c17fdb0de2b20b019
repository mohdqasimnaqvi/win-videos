      <script defer>
$(function() {

    const input = document.querySelector('.search-custom input');
    // Filter Items 
    var items = document.querySelectorAll('.card');
    items.forEach(item => item.style.display = 'none');
    input.addEventListener('keyup', filterItems);
    function filterItems(e){
      // convert text to lowercase
      var text = e.target.value.toLowerCase();
      // Get lis
      // Convert to an array
      Array.from(items).forEach(function(item){
        var itemName = item.querySelector('h3').textContent;
        if(itemName.toLowerCase().indexOf(text) != -1 && text != ''){
          item.style.display = 'block';
        } else {
          item.style.display = 'none';
        }
      });
    }
  // Spacial navigation

  var SN = SpatialNavigation;

  SN.init();

  SN.add({
    id: 'menu',
    selector: '.nav__cont a'
  });

  SN.add({
      id: 'search',
        selector: '.search-custom input'
  })

  SN.add({
      id: 'cards',
        selector: '.card__container .card__content '
  })


  SN.add({
    id: 'footer',
    selector: 'footer a'
  });

    
  SN.makeFocusable();
  SN.focus('menu');
});
</script>
